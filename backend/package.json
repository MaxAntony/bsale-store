    "umzug": "^3.0.0-beta.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/preset-env": "^7.15.6",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0"
  }
